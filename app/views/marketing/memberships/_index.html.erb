<%= render :partial => "shared/tab/marketing", :locals => {:selected => "Memberships"} %>
<div style="background-color:#fff; height:500px" class="row title-row">
<div class="navbar">
    <div class="navbar-inner"><h4 class="title system-management-row">Memberships</h4></div>
</div>
<div style="float:left; width: 19%; margin-left: 10px;">
  <i class="icon-plus-sign icon-blue"></i>
  <%= link_to "New Membership", new_marketing_membership_path, :remote => true   %> 
</div>
<div id="tag_keys" style="float:right; background-color:#d9deec; color:#000; width:79%; margin-top: -20px; border-left: 1px solid black; height:435px; overflow: auto;  ">
  &nbsp;&nbsp;<h4 style="margin-left: 20px; margin-top: -5px; font-weight: bold; font-size: 18px;">Memberships</h4>
  <% @marketing_memberships.each do |marketing_membership| %>
    <ul>
      <li>
        <%= marketing_membership.owner.first_name %>
        &nbsp;<%= link_to "Edit", edit_marketing_membership_path(marketing_membership.id), :remote => true %>|
              <%= link_to "Destroy", marketing_membership, method: :delete, :remote => true %>
      </li>
    </ul>
  <% end %><br />
 </div></div>

