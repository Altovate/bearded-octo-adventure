<%= form_for @phone, url: '/phone/', :remote => true do |f| %>

  <%= f.hidden_field :contact_info_id, :value => @contact_info.id %>

  <%= render :partial => "form", :locals => {:f=>f}%>

  <div class="row">
    <div class="span3"></div>
    <div class="span3">
      <button type="button" id="submit<%= get_contact_info_for %>-phone-form" class="btn btn-primary ajax">Add</button>
    </div>
  </div>
<% end %>
