<%= render :partial => "shared/tab/contracts", :locals => {:selected => "Licenses"} %>
<div style="background-color:#fff; height:500px">
<div class="navbar">
  <div class="navbar-inner"><h4 style="color:black">Licenses</h4></div>
</div>
<div style="float:left; width:175px;">
  <br /><br /><br /><br /><br /><br /><br /><br /><i class="icon-plus-sign icon-blue"></i>
    <%= link_to "New License", new_usage_license_path, :remote => true   %>
 </div>
<div id="tag_keys" style="float:right; background-color:#d9deec; color:#000; width:80%; height:435px; overflow: auto;  ">
    &nbsp;&nbsp;<b>Usage Licenses</b>
  <% @usage_licenses.each do |usage_license| %>
  <ul>
      <li>
            <%= usage_license.user.try(:first_name) %>
      &nbsp;<%= link_to "Edit", edit_usage_license_path(usage_license.id), :remote => true %>|
            <%= link_to "Destroy", usage_license, method: :delete, :remote => true %>
      </li>
    </ul>
  <% end %><br />
  </div>
</div>
